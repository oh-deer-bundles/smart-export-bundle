{
    "name": "oh-deer-bundles/smart-export-bundle",
    "description": "Symfony Bundle helps for export data to file",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.1.3",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "ext-zip": "*",
        "phpoffice/phpspreadsheet": "^1.18",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/orm-pack": "^2.1",
        "symfony/string": "^5.4",
        "symfony/validator": "^5.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4",
        "symfony/browser-kit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Odb\\SmartExportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Odb\\SmartExportBundle\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fabrice GARES"
        }
    ]
}
